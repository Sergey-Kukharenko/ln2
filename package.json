    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.2.0",
