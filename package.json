{
  "name": "london_front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build -m",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "pm2:start": "pm2 start pm2.config.js",
    "pm2:restart": "pm2 restart pm2.config.js",
    "pm2:stop": "pm2 stop pm2.config.js",
    "pm2:delete": "pm2 delete pm2.config.js",
    "pm2": "pm2",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:ts": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:prettier",
    "lint:write": "prettier --write --list-different . && yarn lint:js --fix",
    "pre-commit": "yarn lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "yarn lint:write"
  },
